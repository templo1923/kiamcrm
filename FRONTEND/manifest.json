{
   "action": {
      "default_icon": "label/icons/icon.png"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "css": [ "label/css/waspeed.css", "content/assets/css/style.css", "content.css" ],
      "js": [ "content/index.js" ],
      "matches": [ "https://web.whatsapp.com/*" ]
   } ],
   "description": "KiamberCRM PRO es un CRM para WhatsApp Web. Organiza tus chats y optimiza tu negocio en WhatsApp Web.",
   "icons": {
      "128": "label/icons/icon.png"
   },
   "key": "",
   "manifest_version": 3,
   "name": "KiamberCRM PRO",
   "permissions": [ "unlimitedStorage", "storage", "alarms", "tabs" ],
   
   "host_permissions": [
      "https://web.whatsapp.com/*",
      "https://*.wascript.com.br/*", 
      "https://catalogo.alwaysdata.net/*"
   ],

   "version": "7.4.2.14",
   "web_accessible_resources": [ {
      "matches": [ "https://web.whatsapp.com/*" ],
      "resources": [ "*.js", "*.css", "*.png", "*.mp3", ".ttf", "label/*.json", "label/*.png" ]
   } ]
}